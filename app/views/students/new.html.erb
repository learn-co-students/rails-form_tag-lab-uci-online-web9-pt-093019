<h1>Student Form</h1>

<form action="<%= students_path %>" method="POST">
    <label>First Name:</label><br>
    <input type="text" name="student[first_name]"><br>
    <label>Last Name:</label><br>
    <input type="text" name="student[last_name]"><br>
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <input type="submit" value="Submit Student">
</form>

<%= session[:form_params] if session[:form_params] %>

